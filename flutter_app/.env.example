# Copy to .env and fill in values. Do not commit .env.

# Firebase (required for web; get from Firebase Console → Project settings → Your apps → Web)
FIREBASE_API_KEY=
FIREBASE_APP_ID=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_PROJECT_ID=
FIREBASE_AUTH_DOMAIN=
FIREBASE_STORAGE_BUCKET=

# Optional: Unsplash API key for swipe card images (https://unsplash.com/developers).
# If omitted, app uses a fallback image service.
UNSPLASH_ACCESS_KEY=
